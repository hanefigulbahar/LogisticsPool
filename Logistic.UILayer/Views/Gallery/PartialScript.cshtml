<script src="~/Templates/ui/web/js/SmoothScroll.min.js"></script>
<script type="text/javascript" src="~/Templates/ui/web/js/move-top.js"></script>
<script type="text/javascript" src="~/Templates/ui/web/js/easing.js"></script>
<script type="text/javascript">
	jQuery(document).ready(function($) {
		$(".scroll").click(function(event){
			event.preventDefault();
			$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
		});
	});
</script>
